{
  "name": "theTrustedBay",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Removes untrustworthy search results from thepiratebay.org",
  "icons": {
    "128": "icons/skull-green-md.png"
  },
  "permissions": [
    "https://thepiratebay.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://thepiratebay.org/browse/*",
        "https://thepiratebay.org/search/*"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/trusted.js"
      ]
    }
  ]
}
